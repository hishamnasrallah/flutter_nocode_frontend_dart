DataSource _$DataSourceFromJson(Map<String, dynamic> json) => DataSource(
      id: (json['id'] as num).toInt(),
      application: (json['application'] as num).toInt(),
      name: json['name'] as String,
      dataSourceType: json['dataSourceType'] as String,
      baseUrl: json['baseUrl'] as String?,
      endpoint: json['endpoint'] as String?,
      method: json['method'] as String,
      headers: json['headers'] as String?,
      useDynamicBaseUrl: json['useDynamicBaseUrl'] as bool,
      fields: (json['fields'] as List<dynamic>?)
          ?.map((e) => DataSourceField.fromJson(e as Map<String, dynamic>))
          .toList(),
      fieldsCount: (json['fieldsCount'] as num?)?.toInt(),
      createdAt: DateTime.parse(json['createdAt'] as String),
      updatedAt: DateTime.parse(json['updatedAt'] as String),
    );

Map<String, dynamic> _$DataSourceToJson(DataSource instance) =>
    <String, dynamic>{
      'id': instance.id,
      'application': instance.application,
      'name': instance.name,
      'dataSourceType': instance.dataSourceType,
      'baseUrl': instance.baseUrl,
      'endpoint': instance.endpoint,
      'method': instance.method,
      'headers': instance.headers,
      'useDynamicBaseUrl': instance.useDynamicBaseUrl,
      'fields': instance.fields,
      'fieldsCount': instance.fieldsCount,
      'createdAt': instance.createdAt.toIso8601String(),
      'updatedAt': instance.updatedAt.toIso8601String(),
    };
