Application _$ApplicationFromJson(Map<String, dynamic> json) => Application(
      id: (json['id'] as num).toInt(),
      name: json['name'] as String,
      description: json['description'] as String?,
      packageName: json['packageName'] as String,
      version: json['version'] as String,
      themeId: (json['themeId'] as num?)?.toInt(),
      theme: json['theme'] == null
          ? null
          : AppTheme.fromJson(json['theme'] as Map<String, dynamic>),
      buildStatus: json['buildStatus'] as String,
      apkFile: json['apkFile'] as String?,
      sourceCodeZip: json['sourceCodeZip'] as String?,
      createdAt: DateTime.parse(json['createdAt'] as String),
      updatedAt: DateTime.parse(json['updatedAt'] as String),
      screensCount: (json['screensCount'] as num?)?.toInt(),
      lastBuild: json['lastBuild'] as Map<String, dynamic>?,
      statistics: json['statistics'] as Map<String, dynamic>?,
    );

Map<String, dynamic> _$ApplicationToJson(Application instance) =>
    <String, dynamic>{
      'id': instance.id,
      'name': instance.name,
      'description': instance.description,
      'packageName': instance.packageName,
      'version': instance.version,
      'themeId': instance.themeId,
      'theme': instance.theme,
      'buildStatus': instance.buildStatus,
      'apkFile': instance.apkFile,
      'sourceCodeZip': instance.sourceCodeZip,
      'createdAt': instance.createdAt.toIso8601String(),
      'updatedAt': instance.updatedAt.toIso8601String(),
      'screensCount': instance.screensCount,
      'lastBuild': instance.lastBuild,
      'statistics': instance.statistics,
    };
